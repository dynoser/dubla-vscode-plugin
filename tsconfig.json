{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "target": "ES2020",
    "lib": [
			"ES2020"
		],
    "module": "CommonJS",
    "rootDir": "./src",
    "outDir": "./out",
    "declaration": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
		"node_modules",
		"backup",
		"out",
		"dist",
    "JavaScript"
	]
}
